{
  "name": "Handle Pixel",
  "description": "Use the context menu to search a different country's Google",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["contextMenus", "storage", "tabs", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {}
}
